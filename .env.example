# VoyAIger Backend Environment Variables

# API Keys (Required)
GEMINI_API_KEY=your_gemini_api_key_here
GOOGLE_PLACES_API_KEY=your_google_places_api_key_here
XOTELO_API_KEY=your_rapidapi_key_here

# Supabase Configuration (Required for Auth)
SUPABASE_URL=your_supabase_project_url_here
SUPABASE_KEY=your_supabase_anon_key_here

# JWT Configuration (Required for Auth)
JWT_SECRET_KEY=your_jwt_secret_key_here
JWT_ALGORITHM=HS256
JWT_ACCESS_TOKEN_EXPIRE_MINUTES=1440

# Application Settings (Optional - defaults shown)
ENV=production
LOG_LEVEL=info
REQUEST_TIMEOUT_SECONDS=60
MAX_PREFERENCES_LENGTH=500
MAX_CITY_LENGTH=100

# Model Settings (Optional - defaults shown)
# MODEL_NAME=gemini-2.5-flash-lite
# MODEL_TEMPERATURE=0.2

# Budget Validation (Optional - defaults shown)
# MIN_BUDGET=100.0
# MAX_BUDGET=100000.0
# MAX_TRIP_DAYS=7
